{#
/**
 * @file
 * Theme override to display primary and secondary local tasks.
 *
 * Available variables:
 * - primary: HTML list items representing primary tasks.
 * - secondary: HTML list items representing primary tasks.
 *
 * Each item in these variables (primary and secondary) can be individually
 * themed in menu-local-task.html.twig.
 */
#}
{% if primary %}
  <nav class="tasks primary" aria-labelledby="primary-tabs-label">
    <h2 id="primary-tabs-label" class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
    <ul>{{ primary }}</ul>
  </nav>
{% endif %}
{% if secondary %}
  <nav class="tasks secondary" aria-labelledby="secondary-tabs-label">
    <h2 id="secondary-tabs-label" class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
    <ul>{{ secondary }}</ul>
  </nav>
{% endif %}
